<?xml version='1.0' ?>

<launch>
  <arg name="use_sim_time" default="false"/>

  <!-- Common launch -->
  <include file="$(find-pkg-share fyp)/common.launch.xml">
    <arg name="use_sim_time" value="$(var use_sim_time)"/>
    <arg name="viz_config_file" value ="$(find-pkg-share fyp)/include/simple_I_travel/simple_I_travel.rviz"/>
    <arg name="config_file" value="$(find-pkg-share fyp_maps)/simple_I_travel/simple_I_travel.building.yaml"/>
    <!-- <arg name="dashboard_config_file" value="$(find-pkg-share rmf_demos_dashboard_resources)/simple_I_travel/dashboard_config.json"/> -->
  </include>

  <!-- TinyRobot fleet adapter -->
  <group>
    <include file="$(find-pkg-share fyp_fleet_adapter)/launch/fleet_adapter.launch.xml">
      <arg name="use_sim_time" value="$(var use_sim_time)"/>
      <arg name="nav_graph_file" value="$(find-pkg-share fyp_maps)/maps/simple_I_travel/nav_graphs/0.yaml" />
      <arg name="config_file" value="$(find-pkg-share fyp)/config/simple_I_travel/tinyRobot_config.yaml"/>
    </include>
  </group>
</launch>